# Example environment variables for Codex CLI
# Required
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Optional configuration (e.g., for organizations or Azure deployments)
OPENAI_ORGANIZATION=YOUR_OPENAI_ORGANIZATION_ID_HERE
OPENAI_API_BASE=https://api.openai.com
OPENAI_API_TYPE=azure
OPENAI_API_VERSION=2023-03-15-preview

# Jira CLI configuration (see https://github.com/ankitpokhrel/jira-cli#configuration)
JIRA_API_TOKEN=your-api-token
JIRA_CLOUD_ENDPOINT=https://your-domain.atlassian.net
JIRA_CLOUD_USER_EMAIL=you@example.com
JIRA_CLOUD_PROJECT=YOUR_PROJECT_KEY
JIRA_CLOUD_BOARD=YOUR_BOARD_NAME
